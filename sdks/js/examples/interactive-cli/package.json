{
  "name": "@buildonspark/interactive-cli",
  "version": "0.0.5",
  "private": true,
  "description": "Interactive Ink CLI example for Spark SDK",
  "type": "module",
  "main": "dist/cli.js",
  "scripts": {
    "build": "rm -rf dist && mkdir -p dist && babel cli.tsx --extensions \".ts,.tsx\" --out-file dist/cli.js",
    "start": "node dist/cli.js",
    "format:fix": "prettier . --write",
    "format": "prettier . --check",
    "types": "tsc --noEmit"
  },
  "dependencies": {
    "@buildonspark/spark-sdk": "0.6.1",
    "ink": "^6.6.0",
    "react": "19.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.27.1",
    "@babel/core": "^7.27.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@types/node": "^22.13.2",
    "@types/react": "^19.0.10",
    "prettier": "^3.7.4",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=18"
  }
}
